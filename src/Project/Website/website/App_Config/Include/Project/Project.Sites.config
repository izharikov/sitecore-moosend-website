<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
    <sitecore>
        <sites>
            <site name="Moosend"
                  patch:before="site[@name='website']"
                  hostName="$(env:CD_HOST)|$(env:SITE_HOST)"
                  targetHostName="$(env:SITE_HOST)"
                  language="en"
                  enableItemLanguageFallback="true"
                  enableFieldLanguageFallback="true"
                  contentLanguage="en"
                  database="$(env:DATABASE)"
                  virtualFolder="/"
                  physicalFolder="/"
                  rootPath="/sitecore/content/Websites/Moosend"
                  startItem="/Home"
                  allowDebug="true"
                  enableRedirects="true"
                  cacheHtml="true"
                  glassCache="false"
                  htmlCacheSize="50MB"
                  registryCacheSize="0"
                  viewStateCacheSize="0"
                  xslCacheSize="25MB"
                  filteredItemsCacheSize="10MB"
                  enablePreview="true"
                  enableWebEdit="true"
                  enableDebugger="true"
                  disableClientData="false"
                  cacheRenderingParameters="true"
                  renderingParametersCacheSize="10MB"
                  dictionaryPath=""
                  applicationId=""
                  domain="extranet"
                  explicitConsentForTrackingIsRequired="true"
            />
        </sites>
    </sitecore>
</configuration>